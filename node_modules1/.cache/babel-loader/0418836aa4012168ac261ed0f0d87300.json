s\">\n    <input class=\"form-control\" type=\"text\" v-model=\"searchQuery\" placeholder=\"Search\" />\n    <div id=\"articles\" v-for=\"item in resultQuery\" :key=\"item.id\">\n        <h2>{{ item.title }}</h2>\n        <article>{{ item.body }}</article>\n    </div>   \n</div>\n\n</template>\n\n<script>\nexport default {\n    name: 'blogs',\n    data() {\n        return{\n            content: {},\n            searchQuery: ''\n        }\n    },\n    methods: {\n        async addContent(){   \n            try {\n            const data = await fetch('https://jsonplaceholder.typicode.com/posts')\n            const objeto = await data.json()\n            console.log(objeto)\n            this.content = objeto\n            } catch (error) {\n            console.log(error)\n            }\n        }\n    },\n    created(){\n        this.addContent()\n    },\n    computed: {\n    resultQuery(){\n      if(this.searchQuery){\n      return this.content.filter((item)=>{\n        return this.searchQuery.toLowerCase().split(' ').every(v => item.title.toLowerCase().includes(v))\n      })\n      }else{\n        return this.content;\n      }\n    }\n  }\n \n\n}\n</script>\n\n<style>\n#blogs{\n    max-width: 1200px;\n    margin: 0 auto;\n}\n#articles {\n    color: #42b983;\n    padding: 20px;\n    padding-top: 5px;\n    margin: 20px;\n    box-sizing: border-box;\n    background: #2c3e50;\n}\n\n</style>"],"sourceRoot":"src/views"}]}